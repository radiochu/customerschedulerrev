<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/18"
      fx:controller="Controller.Login">
    <AnchorPane VBox.vgrow="ALWAYS">
        <Label layoutX="50.0" layoutY="39.0" text="%apptitle">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <TextField fx:id="username" layoutX="55.0" layoutY="103.0" prefHeight="26.0" prefWidth="190.0"/>
        <PasswordField fx:id="password" layoutX="55.0" layoutY="172.0" prefHeight="26.0" prefWidth="190.0"/>
        <Label layoutX="57.0" layoutY="83.0" text="%un">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label layoutX="57.0" layoutY="151.0" text="%pw">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <AnchorPane layoutX="14.0" layoutY="254.0" prefHeight="58.0" prefWidth="190.0">
            <Label fx:id="timezone" layoutY="20.0" text="%timezone"/>
        </AnchorPane>
        <ButtonBar layoutX="50.0" layoutY="214.0" prefHeight="40.0" prefWidth="200.0">
            <buttons>
                <Button mnemonicParsing="false" onAction="#onSubmit" text="%submit"/>
                <Button layoutX="135.0" layoutY="17.0" mnemonicParsing="false" onAction="#onCancel" text="%cancel"/>
            </buttons>
        </ButtonBar>
    </AnchorPane>
</VBox>
